{
  "name": "osu-js",
  "version": "0.1.0",
  "description": "Prototype port of Osu! to JavaScript",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier": "prettier --single-quote --print-width 100 --tab-width 4 --use-tabs \"./src/**/*.js\" --write",
    "dev": "webpack --config webpack.config.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "author": "Sam Lynch",
  "license": "MIT",
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [],
    "rules": {
      "no-console": "off",
      "no-undef": 0,
      "no-unused-vars": "off",
      "max-len": [
        1,
        120,
        2,
        {
          "ignoreComments": true
        }
      ],
      "prop-types": [
        0
      ]
    },
    "extends": [
      "eslint:recommended"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.0.15",
    "osu-json": "^0.8.2",
    "pixi.js": "^4.7.3",
    "prop-types": "^15.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.2",
    "babel-preset-stage-0": "^6.0.15",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.10",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.0.6",
    "pixi-sound": "^2.0.2",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.20.3",
    "webpack": "^3.10.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^2.4.1"
  }
}
